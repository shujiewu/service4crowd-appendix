{
	"name": "service-model-detection",
	"serviceType": "MachineService",
	"atomicServices": [{
		"name": "trainModel",
		"version": 1,
		"description": "trainModel",
		"url": "/annotation/model/train",
		"async": false,
		"method": "POST",
		"queryParameters": [],
		"uriParameters": [],
		"body": [{
			"name": "modelName",
			"type": "string",
			"description": "modelName",
			"default": "resnet50fpn"
		}, {
			"name": "gpuNum",
			"type": "int",
			"description": "gpuNum",
			"default": 1
		}, {
			"name": "maxIter",
			"type": "int",
			"description": "maxIter",
			"default": 40000
		}, {
			"name": "weightDecay",
			"type": "double",
			"description": "weightDecay",
			"default": 0.0001
		}, {
			"name": "steps",
			"type": "list",
			"description": "steps",
			"default": [0, 20000, 40000]
		}, {
			"name": "lrs",
			"type": "list",
			"description": "lrs",
			"default": [0, 0.005, 0.0005]
		}, {
			"name": "dataSetName",
			"type": "string",
			"description": "dataSetName",
			"default": "voc2007"
		}, {
			"name": "imageIdList",
			"type": "list",
			"description": "imageIdList"
		}, {
			"name": "trainingItemList",
			"type": "list",
			"description": "trainingItemList"
		}, {
			"name": "simulate",
			"type": "boolean",
			"description": "simulate",
			"default": false
		}],
		"response": [{
			"status": 200,
			"description": "",
			"parameters": [{
				"name": "trainingId",
				"type": "string",
				"description": "trainingId"
			}, {
				"name": "modelPkl",
				"type": "string",
				"description": "modelPkl"
			}, {
				"name": "modelConfig",
				"type": "string",
				"description": "modelConfig"
			}]
		}]
	}, {
		"name": "selectData",
		"version": 1,
		"description": "selectData",
		"url": "/annotation/model/select/active",
		"async": false,
		"method": "POST",
		"queryParameters": [],
		"uriParameters": [],
		"body": [{
			"name": "activeMethodName",
			"type": "string",
			"description": "activeMethodName",
			"default": "fpn"
		}, {
			"name": "modelPkl",
			"type": "string",
			"description": "modelPkl"
		}, {
			"name": "modelConfig",
			"type": "string",
			"description": "modelConfig"
		}, {
			"name": "dataSetName",
			"type": "string",
			"description": "dataSetName",
			"default": "voc2007"
		}, {
			"name": "imageIdList",
			"type": "list",
			"description": "imageIdList"
		}, {
			"name": "selectNum",
			"type": "int",
			"description": "selectNum"
		}],
		"response": [{
			"status": 200,
			"description": "",
			"parameters": [{
				"name": "inferenceId",
				"type": "string",
				"description": "inferenceId"
			}, {
				"name": "annotationList",
				"type": "list",
				"description": "annotationList"
			}, {
				"name": "selectImageIdList",
				"type": "list",
				"description": "selectImageIdList"
			}, {
				"name": "remainImageIdList",
				"type": "list",
				"description": "remainImageIdList"
			}, {
				"name": "remainImageNum",
				"type": "int",
				"description": "remainImageNum"
			}]
		}]
	}]
}