name: RandomSelect
entry_points:
  main:
    parameters:
      imageData: {type: file, default: ""}
      selectSize: {type: int, default: 1}
      seed: {type: int, default: 10}
      storage_dir: {type: output, default: ""}
    command: "python random_select.py {selectSize} {imageData} {seed} {storage_dir}"
